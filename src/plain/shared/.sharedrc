{
  "settings": {
    "maxFileSize": 150,
    "optimizations": {
      "css": false,
      "image": true,
      "html": false,
      "js": false
    },
    "useOriginalFileNames": true,
    "entry": {
      "js": "./script/main.js",
      "html": "./index.hbs"
    },
    "fonts": [
      {
        "sources": [
          "./fonts/OpenSans-Regular.woff",
          "./fonts/OpenSans-Regular.woff2"
        ],
        "subset": {
          "glyphs": [
            "content.title"
          ]
        }
      },
      {
        "sources": [
          "./fonts/OpenSans-Bold.woff",
          "./fonts/OpenSans-Bold.woff2"
        ],
        "subset": {
          "glyphs": [
            "content.cta"
          ]
        }
      }
    ]
  },
  "content": {
    "css": "./css/style.scss",
    "clickTag": "https://www.google.com",
    "bgcolor": "#FFFFFF"
  }
}